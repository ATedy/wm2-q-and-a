{
  "name": "starter-kit",
  "version": "0.0.1",
  "description": "Starter kit for full-stack JavaScript projects",
  "private": true,
  "scripts": {
    "prebuild": "npm run cleanup",
    "build": "npm run build:server && npm run build:client",
    "build:client": "webpack --config client/webpack/prod.config.js",
    "build:server": "babel server --out-dir dist",
    "cleanup": "rimraf ./dist/*",
    "dev": "concurrently -k -n \"client,server\" \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "webpack serve --config client/webpack/dev.config.js",
    "dev:server": "cross-env PORT=3100 nodemon --inspect --delay 500ms --watch server --exec babel-node server/server.js",
    "lint": "eslint .",
    "preserve": "npm run build",
    "serve": "npm start",
    "start": "node dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/textbook/starter-kit.git"
  },
  "author": "Jonathan Sharpe <mail@jonrshar.pe>",
  "contributors": [],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/textbook/starter-kit/issues"
  },
  "homepage": "https://github.com/textbook/starter-kit#readme",
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.2",
    "@babel/node": "^7.14.2",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-runtime": "^7.14.2",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@codeyourfuture/eslint-config-standard": "^2.1.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.4",
    "eslint": "^7.26.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "html-webpack-tags-plugin": "^3.0.1",
    "nodemon": "^2.0.7",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "rimraf": "^3.0.2",
    "style-loader": "^2.0.0",
    "wait-on": "^5.3.0",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.0",
    "@ckeditor/ckeditor5-build-classic": "^28.0.0",
    "@ckeditor/ckeditor5-react": "^3.0.2",
    "acorn": "^8.4.1",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^5.0.1",
    "ckeditor": "^4.12.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "helmet": "^4.6.0",
    "html-react-parser": "^1.2.7",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg": "^8.6.0",
    "react-bootstrap": "^1.6.1",
    "react-tsparticles": "^1.29.2",
    "tsparticles": "^1.29.2"
  },
  "engines": {
    "node": "^10 || ^12 || ^14 || ^16"
  }
}
